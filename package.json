{
  "name": "piling.js-vue",
  "version": "0.1.0",
  "description": "Template for using Piling.js with Vue.js v3",
  "keywords": [
    "piling.js",
    "vue",
    "vue3"
  ],
  "author": [
    {
      "name": "Fritz Lekschas",
      "url": "https://lekschas.de"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/piling.js-vue.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build; touch dist/.nojekyll; gh-pages -d dist -t --git git"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "piling.js": "^0.7.12",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "gh-pages": "^3.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
